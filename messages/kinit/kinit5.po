#. +> trunk5
#: kdeinit/kinit.cpp:455
#, kde-format
msgid ""
"Unable to start new process.\n"
"The system may have reached the maximum number of open files possible or the maximum number of open files that you are allowed to use has been reached."
msgstr ""
"Ní féidir próiseas nua a thosú.\n"
"Seans go bhfuil líon uasta na gcomhad ar féidir bheith oscailte sroichte, nó líon uasta na gcomhad a bhfuil cead agat iad a úsáid."

#. +> trunk5
#: kdeinit/kinit.cpp:477
#, kde-format
msgid ""
"Unable to create new process.\n"
"The system may have reached the maximum number of processes possible or the maximum number of processes that you are allowed to use has been reached."
msgstr ""
"Ní féidir próiseas nua a thosú.\n"
"Seans go bhfuil líon uasta na bpróiseas sroichte, nó líon uasta na bpróiseas a bhfuil cead agat iad a úsáid."

#. +> trunk5
#: klauncher/klauncher.cpp:418
#, kde-format
msgid "KDEInit could not launch '%1'"
msgstr "Níorbh fhéidir le KDEInit '%1' a thosú"

#. +> trunk5
#: klauncher/klauncher.cpp:594 klauncher/klauncher.cpp:608
#, kde-format
msgid "Could not find service '%1'."
msgstr "Ní bhfuarthas seirbhís '%1'."

#. +> trunk5
#: klauncher/klauncher.cpp:626
#, kde-format
msgid "Service '%1' must be executable to run."
msgstr "Caithfidh seirbhís '%1' a bheith inrite chun é a rith."

#. +> trunk5
#: klauncher/klauncher.cpp:628 klauncher/klauncher.cpp:689
#, kde-format
msgid "Service '%1' is malformatted."
msgstr "Tá seirbhís '%1' formáidithe go mícheart."

#. +> trunk5
#: klauncher/klauncher.cpp:784
#, kde-format
msgid "Launching %1"
msgstr "%1 á thosú"

#. +> trunk5
#: klauncher/klauncher.cpp:965
#, kde-format
msgid "Unknown protocol '%1'.\n"
msgstr "Prótacal anaithnid '%1'.\n"

#. +> trunk5
#: klauncher/klauncher.cpp:971
#, kde-format
msgid "Could not find the '%1' plugin.\n"
msgstr "Ní bhfuarthas an breiseán '%1'.\n"

#. +> trunk5
#: klauncher/klauncher.cpp:1044
#, kde-format
msgid "Error loading '%1'."
msgstr "Earráid ag luchtú '%1'."

#. +> trunk5
#: klauncher/klauncher_main.cpp:136
#, kde-format
msgid ""
"klauncher: This program is not supposed to be started manually.\n"
"klauncher: It is started automatically by kdeinit5.\n"
msgstr ""
"klauncher: Ní ceart duit an clár seo a thosú tú féin.\n"
"klauncher: Tosaíonn kdeinit5 é go huathoibríoch.\n"
